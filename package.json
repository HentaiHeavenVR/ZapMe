{
  "name": "zapme",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "sync": "svelte-kit sync",
    "build": "rimraf src/lib/api && openapi-generator-cli generate && vite build",
    "build:app": "vite build",
    "package": "svelte-kit package",
    "preview": "vite preview",
    "test:ci": "run-s -l test:ui test:unit:ci",
    "test:ui": "playwright test",
    "test:unit": "vitest",
    "test:unit:ci": "vitest --run",
    "coverage": "vitest run --coverage",
    "check": "svelte-kit sync && svelte-check",
    "check:watch": "svelte-kit sync && svelte-check --watch",
    "lint": "prettier --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --write --plugin-search-dir=. .",
    "clean": "rimraf src/lib/api && rimraf .svelte-kit"
  },
  "devDependencies": {
    "@fontsource/montserrat": "^4.5.14",
    "@openapitools/openapi-generator-cli": "^2.6.0",
    "@playwright/test": "^1.32.3",
    "@sentry/svelte": "^7.49.0",
    "@sentry/tracing": "^7.49.0",
    "@sveltejs/adapter-auto": "^2.0.1",
    "@sveltejs/adapter-cloudflare": "^2.2.0",
    "@sveltejs/kit": "^1.15.8",
    "@sveltejs/vite-plugin-svelte": "^2.1.0",
    "@testing-library/svelte": "^3.2.2",
    "@tsconfig/svelte": "^4.0.1",
    "@types/jest": "^29.5.1",
    "@vitest/coverage-c8": "^0.30.1",
    "@vitest/ui": "^0.30.1",
    "cross-env": "^7.0.3",
    "email-validator": "^2.0.4",
    "eslint": "^8.39.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "http-status-codes": "^2.2.0",
    "jsdom": "^21.1.1",
    "material-symbols": "^0.5.5",
    "npm-check-updates": "^16.10.9",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "prettier-plugin-svelte": "^2.10.0",
    "rimraf": "^5.0.0",
    "svelte": "^3.58.0",
    "svelte-check": "^3.2.0",
    "svelte-i18n": "^3.6.0",
    "svelte-local-storage-store": "^0.4.0",
    "tslib": "^2.5.0",
    "typescript": "^5.0.4",
    "vite": "^4.3.1",
    "vitest": "^0.30.1"
  },
  "browserslist": {
    "production": [
      "> 0.5%",
      "not dead",
      "not op_mini all",
      "supports es6-module"
    ],
    "development": [
      "last 1 version"
    ]
  },
  "prettier": {
    "printWidth": 80,
    "tabWidth": 2,
    "useTabs": false,
    "semi": true,
    "singleQuote": true,
    "quoteProps": "as-needed",
    "trailingComma": "es5",
    "bracketSpacing": true,
    "bracketSameLine": false,
    "arrowParens": "always",
    "requirePragma": false,
    "insertPragma": false,
    "proseWrap": "preserve",
    "htmlWhitespaceSensitivity": "css",
    "endOfLine": "lf",
    "plugins": [
      "prettier-plugin-svelte"
    ]
  }
}
