{
  "extends": "./.svelte-kit/tsconfig.json",
  "compilerOptions": {
    "allowJs": false,
    "esModuleInterop": true,
    "sourceMap": false,
    "strict": true,

    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "node",
    "resolveJsonModule": true,

    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "importsNotUsedAsValues": "remove",
    "noFallthroughCasesInSwitch": false,

    "types": [
      "@sveltejs/kit",
      "vitest/globals",
    ]
  }
}
