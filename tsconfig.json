{
  "extends": "./.svelte-kit/tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",

    "allowJs": false,
    "esModuleInterop": true,
    "sourceMap": false,
    "strict": true,

    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "node16",
    "resolveJsonModule": true,

    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "importsNotUsedAsValues": "remove",
    "noFallthroughCasesInSwitch": false,
  }
}
