{
  "extends": "./.svelte-kit/tsconfig.json",
  "compilerOptions": {
    "target": "ES2020",

    "allowJs": false,
    "esModuleInterop": true,
    "sourceMap": false,
    "strict": true,

    "noUnusedLocals": true,
    "noImplicitReturns": true,
    "noUnusedParameters": true,
    "allowUnreachableCode": false,
    "importsNotUsedAsValues": "remove",
    "noFallthroughCasesInSwitch": false
  }
}
